<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="440.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.StartPageController">
   <children>
      <ImageView fitHeight="440.0" fitWidth="600.0" pickOnBounds="true">
       <image>
           <Image url="@disco-dba865f1.png" />
       </image>
      </ImageView>
      <Label layoutX="136.0" layoutY="41.0" text="Welcome To Timeline">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <Label layoutX="154.0" layoutY="255.0" text="Please select your user type:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="singleUserButton" layoutX="149.0" layoutY="326.0" mnemonicParsing="false" onAction="#switchToHomepage" style="-fx-background-color: opaque; -fx-border-color: black;" text="Single User">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="multiUserButton" layoutX="323.0" layoutY="326.0" mnemonicParsing="false" onAction="#switchToHomepage" style="-fx-background-color: opaque; -fx-border-color: black;" text="Multi User">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
